pareja(X, Y) :- pareja(Y, X).

hijo(X, Y) :- padre(Y, X).
hijo(X, Y) :- madre(Y, X).

hermano_a(X, Y) :-
    X \= Y,
    (padre(P, X), padre(P, Y);
     madre(M, X), madre(M, Y)).

abuelo_a(X, N) :-
    (padre(X, P); madre(X, P)),
    (padre(P, N); madre(P, N)).

bisnieto_a(BN, BA) :-
    (padre(P, BN); madre(P, BN)),
    (padre(GP, P); madre(GP, P)),
    (padre(BA, GP); madre(BA, GP)).

sobrino_a(S, T) :-
    (padre(P, S); madre(P, S)),
    hermano_a(P, T).

nuera(N, S) :-
    pareja(N, H),
    (padre(S, H); madre(S, H)).

tio_abuelo_a(TA, N) :-
    abuelo_a(A, N),
    hermano_a(TA, A).



descendiente(Persona1, Persona2) :-
    padre(Persona1, Persona2);
    madre(Persona1, Persona2).

descendiente(Persona1, Persona2) :-
    (padre(Persona1, Hijo); madre(Persona1, Hijo)),
    descendiente(Hijo, Persona2).